{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "makerblocks/template",
  "version": "0.1.0",
  "title": "Template Block",
  "category": "makerblocks",
  "icon": "insert",
  "description": "Template block for creating new custom blocks",
  "example": {},
  "attributes": {
    "heading": {
      "type": "string",
      "default": "Block Heading"
    },
    "content": {
      "type": "string",
      "default": "Block content goes here."
    },
    "showFeature": {
      "type": "boolean",
      "default": true
    },
    "numericValue": {
      "type": "number",
      "default": 100
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "makerblocks",
  "editorScript": "file:./index.js",
  "render": "file:./render.php"
}